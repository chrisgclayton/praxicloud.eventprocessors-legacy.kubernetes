    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <AssemblyVersion>1.0.11.0</AssemblyVersion>
    <FileVersion>1.0.11.0</FileVersion>
    <Copyright>Â© Christopher Clayton. All rights reserved.</Copyright>
    <RepositoryUrl>https://github.com/chrisgclayton/praxicloud.eventprocessors-legacy.kubernetes</RepositoryUrl>
    <PackageProjectUrl>https://github.com/chrisgclayton/praxicloud.eventprocessors-legacy.kubernetes</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Messaging.EventHubs" Version="5.2.0" />
    <PackageReference Include="PraxiCloud-Core-Kubernetes" Version="2.3.1" />
    <PackageReference Include="PraxiCloud-Core-Metrics-Prometheus" Version="2.3.2" />
    <PackageReference Include="PraxiCloud-Core-Security-KeyVault" Version="2.3.0" />
    <PackageReference Include="PraxiCloud-EventProcessors-Legacy" Version="1.2.0" />
